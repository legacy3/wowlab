{
  "client": {
    "token": {
      "hmac_secret_key": ""
    },
    "allowed_origins": [
      "https://wowlab.gg",
      "https://*.wowlab.gg"
    ]
  },

  "http_api": {
    "key": ""
  },

  "admin": {
    "enabled": true
  },

  "health": {
    "enabled": true
  },

  "prometheus": {
    "enabled": true
  },

  "broker": {
    "enabled": true,
    "type": "nats",
    "nats": {
      "url": "nats://wowlab-nats.internal:4222"
    }
  },

  "presence_manager": {
    "enabled": true,
    "type": "redis",
    "redis": {}
  },

  "channel": {
    "namespaces": [
      {
        "name": "nodes",
        "allow_subscribe_for_client": true,
        "presence": true,
        "join_leave": true,
        "force_push_join_leave": true,
        "allow_presence_for_subscriber": true
      },
      {
        "name": "jobs",
        "allow_subscribe_for_client": true
      },
      {
        "name": "chunks",
        "allow_subscribe_for_client": true
      }
    ]
  }
}
