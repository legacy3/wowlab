{
  "name": "@wowlab/rotation",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./build/index.js",
    "./*": "./build/*.js",
    "./internal/*": null
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "vite build && tsc-alias -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "ncu": "ncu -u"
  },
  "dependencies": {
    "@wowlab/core": "workspace:*",
    "@wowlab/services": "workspace:*",
    "effect": "^3.19.4",
    "immutable": "^5.1.4"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.16",
    "vite-plugin-dts": "^4.5.4"
  }
}
