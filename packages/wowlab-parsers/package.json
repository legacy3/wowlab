{
  "name": "@wowlab/parsers",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./build/index.js",
    "./*": "./build/*.js",
    "./internal/*": null
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "vite build && tsc-alias -p tsconfig.json",
    "test": "vitest run",
    "test:ui": "vitest --ui --open",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "ncu": "ncu -u"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "effect": "^3.19.4"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "tsc-alias": "^1.8.16",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
