{
  "name": "@packages/innocent-services",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./build/index.js",
    "./*": "./build/*.js",
    "./internal/*": null
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "vite build && tsc-alias -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ncu": "ncu -u"
  },
  "dependencies": {
    "@packages/innocent-domain": "workspace:*",
    "@packages/innocent-schemas": "workspace:*",
    "@packages/innocent-spellbook": "workspace:*",
    "effect": "^3.19.3",
    "immutable": "^5.1.4",
    "microdiff": "^1.5.0",
    "tinyqueue": "^3.0.0"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.16",
    "vite-plugin-dts": "^4.5.4"
  }
}
